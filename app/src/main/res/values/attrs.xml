<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="layout">
        <attr name="index" format="integer" />
        <attr name="holder_index" format="integer" />
        <attr name="refresh" format="boolean" />
        <attr name="enable" format="boolean" />
        <attr name="page" format="boolean" />
        <attr name="count" format="integer" />
        <attr name="rotate" format="boolean" />
        <attr name="loop" format="integer" />
        <attr name="type">
            <flag name="recycler" value="0x00" />
            <flag name="pager" value="0x10" />
            <flag name="frame" value="0x20" />
            <flag name="gallery" value="0x30" />
            <flag name="flow" value="0x40" />
            <flag name="media" value="0x50" />
        </attr>
    </declare-styleable>

</resources>
